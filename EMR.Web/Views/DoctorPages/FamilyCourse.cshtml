<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
	<link rel="stylesheet" href="~/Scripts/layui/css/layui.css">
	<link rel="stylesheet" href="~/Content/reset.css">
	<link rel="stylesheet" href="~/Content/PerinataPaper.css">
</head>
<body>
	<div class="medrecord">
        <p class="word">计划生育病程记录</p>
        <div class="rigBtn">
             <a class="layui-btn">暂存</a>
             <a class="layui-btn">保存</a>
        </div>
 	</div>
	<div class="contenter">
		<table class="table layui-form num double">
			<tr>
				<td>给药时间</td>
				<td><input type="text" id="test" ></td>
				<td>方法</td>
				<td><input type="text"></td>
				<td>剂量</td>
				<td><input type="text"></td>
			</tr>
			<tr>
				<td colspan="6" class="left"><textarea name="" id="FamilyPlan" style="display:none;"></textarea></td>
			</tr>
			<tr>
				<td>宫缩开始时间</td>
				<td><input type="text" id="test1"></td>
				<td>胎儿排出时间</td>
				<td><input type="text" id="test2"></td>
				<td>引产方式</td>
				<td>
					<input type="checkbox" name="" title="自排" lay-skin="primary" >
					<input type="checkbox" name="" title="钳夹" lay-skin="primary" >
					<input type="checkbox" name="" title="牵引" lay-skin="primary" >
				</td>
			</tr>
			<tr>
				<td>胎盘排出时间</td>
				<td><input type="text" id="test3"></td>
				<td>排出方式</td>
				<td colspan="3">
					<input type="radio" name="normal" title="自排">自排
					<input type="radio" name="normal" title="手取">手取
					<input type="radio" name="normal" title="完整">完整
					<input type="radio" name="normal" title="不完整钳夹">不完整钳夹
					<input type="text" class="others">
				</td>
			</tr>
			<tr>
				<td>失血量</td>
				<td><input type="text">ml</td>
				<td>胎儿</td>
				<td>
					<input type="radio" name="normal" title="男">男
					<input type="radio" name="normal" title="女">女
					<input type="radio" name="normal" title="不详">不详
					<input type="radio" name="normal" title="死">死
					<input type="radio" name="normal" title="活">活
					<input type="text" name="" class="others">
				</td>
				<td>活胎归局</td>
				<td><input type="text"></td>
			</tr>
			<tr>
				<td>辅助用药</td>
				<td colspan="5"><input type="text"></td>
			</tr>
			<tr>
				<td>记录者</td>
				<td><input type="text"></td>
				<td>审核者签名</td>
				<td colspan="3">
					<input type="text" class="organs">
					<a class="layui-btn">签名</a>
					<a class="layui-btn">取消签名</a>
				</td>
			</tr>
			<tr>
				<td colspan="2">
					<input type="checkbox" name="" title="钳刮术" lay-skin="primary" >
					<input type="checkbox" name="" title="清宫术" lay-skin="primary" >
				</td>
				<td>记录</td>
				<td>
					<span>宫腔<input type="text"  class="others">至<input type="text"  class="others">厘米</span>
				</td>
				<td>手术时间</td>
				<td><input type="text" id="test5"></td>
			</tr>
			<tr>
				<td colspan="6" class="left"><textarea name="" id="sec" style="display:none"></textarea></td>
			</tr>
			<tr>
				<td>记录者</td>
				<td><input type="text"></td>
				<td>审核者签名</td>
				<td colspan="3"><input type="text" class="others"><a class="layui-btn">签名</a><a class="layui-btn">取消签名</a></td>
			</tr>
		</table>
	</div>
</body>
<script src="~/Scripts/layui/layui.js"></script>
<script>
layui.use(['form','laydate','layedit'],function(){
	var form = layui.form;
	var laydate = layui.laydate;
	var layedit = layui.layedit;
	laydate.render({
	  elem: '#test',
	  type:'datetime' 
	});
	laydate.render({
	  elem: '#test1',
	  type:'datetime' 
	});
	laydate.render({
	  elem: '#test2',
	  type:'datetime' 
	});
	laydate.render({
	  elem: '#test3',
	  type:'datetime' 
	});
	laydate.render({
	  elem: '#test4',
	  type:'datetime' 
	});
	laydate.render({
	  elem: '#test5',
	  type:'datetime' 
	});
	layedit.build('FamilyPlan',{height:197});
	layedit.build('sec',{height:197});

})
</script>
</html>