                        } else {
                            layer.msg("删除失败", { icon: 5 });
                        }
                    }
                });
            });

            return false;
        });
        //点击打印跳转到打印页面
        $(document).on('click', ".print", function () {
            window.location.href = EWinsBase.__curHostAddress + 'page/DoctorPages/HospitalRecord';
        });
    });




</script>
