            var gmstime = $("#txtGMStime").val();
            var gmsname = $("#txtGMSname").val();
            var gmszz = $("#txtGMSzz").val();
            if (gmszz == "") {
                layer.msg("症状不能为空", { time: 2000 });
            } else {
                $("#divGMlist").show();
                //点击添加按钮时，新添一行
                var oldData = [];
                if (table.cache["tabGMS"] != undefined) { oldData = table.cache["tabGMS"] }
                var data1 = { "time": gmstime, "name": gmsname, "zz": gmszz, "experience": "<span class=\"delGMS\">删除</span>" };
