<script>


    //病人id
    var InpatientId = NewGetQueryString("InpatientId");
    var MedicalRecordState = "0";//是否归档，默认0 归档
    if (InpatientId != "") {
        //根据病人id获取病历信息
        EWinsBase.json("api/Doctor/HospitalRecord/GetInfoByInpatientId", {
            data: { "InpatientId": InpatientId },
            type: "POST",
            async: false,
            usedCache: false,
            success: function (config, data) {
                if (data.data != null) {
                    //如果存在该病人的入院记录
                    EWinsBase.setDataModel($("#divData [data-id]"), data.data);

                    //绑定富文本框的内容需要反转义
                    $("#PresentHistory").val(HTMLDecode(data.data["PresentHistory"]));//反转义现病史
                    $("#PreviousHistory").val(HTMLDecode(data.data["PreviousHistory"]));//反转义既往史
                    $("#SystemReview").val(HTMLDecode(data.data["SystemReview"]));//反转义系统回顾
                    $("#PersonalHistory").val(HTMLDecode(data.data["PersonalHistory"]));//反转义个人史

                    $("#LaboratoryExamination").val(HTMLDecode(data.data["LaboratoryExamination"]));//反转义实验室检查
                    $("#SpecialExamination").val(HTMLDecode(data.data["SpecialExamination"]));//反转义特殊检查
                    $("#PathologicalExamination").val(HTMLDecode(data.data["PathologicalExamination"]));//反转义病理检查
                    $("#OtherContent").val(HTMLDecode(data.data["OtherContent"]));//反转义其他检查

                } else {
                    //如果不存在该病人的入院记录
                    $("#UpdaterName").val(cookie.GetEMRUserCookie("api_token", "USERNAME"));//登陆账号为记录医师
                    $("#RecordTime").val(currentTime());//提交日期
                }
            },
            error: function (error) {

            }
        });
        //获取病人实体
        EWinsBase.json("api/Doctor/MedicalRecordHomePage/GetInpatientInfoById", {
            data: { "InpatientId": NewGetQueryString("InpatientId") },
            type: "POST",
            async: false,
            usedCache: false,
            success: function (config, data) {
                EWinsBase.setDataModel($("#divData [data-id]"), data.data);
                MedicalRecordState = data.data["MedicalRecordState"];
                //判断是否归档，未归档，启用按钮
                if (MedicalRecordState != "1") {
                    $("button.cz").removeAttr("disabled");
                    $("button.cz").removeClass("layui-btn-disabled");                    
                }
            }
        });
        //根据病人id获取体格检查
        EWinsBase.json("api/Doctor/HospitalPhysicalRxam/GetInfoByInpatientId", {
            data: { "InpatientId": InpatientId },
            type: "POST",
            async: false,
            usedCache: false,
            success: function (config, data) {
                if (data.data != null) {
                    //如果存在该病人的入院记录
                    EWinsBase.setDataModel($("#divData [data-id]"), data.data);
                }
            },
            error: function (error) {

            }
        });
    }
    //导入生命体征
