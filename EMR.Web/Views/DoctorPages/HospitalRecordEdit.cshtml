        //根据病人id获取体格检查
        EWinsBase.json("Scripts/VitalSigns.json", {
            data: {  },
            type: "GET",
            async: false,
            usedCache: false,
            success: function (config, data) {
                if (data.data != null) {
                    //如果存在该病人的入院记录
                    EWinsBase.setDataModel($("#divData [data-id]"), data.data);
                }
            },
            error: function (error) {

            }
        });
    });
