					<input type="checkbox" name="" lay-skin="primary" title="脐部护理">
					<input type="checkbox" name="" lay-skin="primary" title="更换吸氧管">
					<input type="checkbox" name="" lay-skin="primary" title="气压治疗">
					<input type="checkbox" name="" lay-skin="primary" title="更换减压装置">
					<input type="checkbox" name="" lay-skin="primary" title="伤口换药">
					<input type="checkbox" name="" lay-skin="primary" title="更换引流瓶">
					<input type="checkbox" name="" lay-skin="primary" title="检查约束部位">
					<input type="checkbox" name="" lay-skin="primary" title="气切护理">
					<input type="checkbox" name="" lay-skin="primary" title="更换床单">
					<input type="checkbox" name="" lay-skin="primary" title="拔除外置留置针">
					<input type="checkbox" name="" lay-skin="primary" title="翻身">
					<input type="checkbox" name="" lay-skin="primary" title="吸痰">
					<input type="checkbox" name="" lay-skin="primary" title="超声药物透入治疗">
					<input type="checkbox" name="" lay-skin="primary" title="高危压疮防范护理">
					<input type="checkbox" name="" lay-skin="primary" title="中药涂擦">
					<input type="checkbox" name="" lay-skin="primary" title="低频脉冲电治疗">
				</td>
			</tr>
			<tr>
				<td>健康教育：</td>
				<td colspan="8">
					<input type="text">
					<a>导入健康宣教</a>
				</td>
			</tr>
			<tr>
				<td>
					<p><a href="javascript:;">病情观察</a></p>
					<p><a href="javascript:;">护理措施</a></p>
					<p><a href="javascript:;">效果评价</a></p>
				</td>
				<td colspan="8" class="clearfix">
					<div class="whrite fl">
						<textarea name="" id="observation" cols="30" rows="10"></textarea>
					</div>
					<div class="listBtn fl">
						<div>
							<a class="layui-btn">检验报告</a>
							<a class="layui-btn">检查报告</a>
						</div>
						<div>
							<a class="layui-btn">专业护理</a>
							<a class="layui-btn">记录模板</a>
						</div>
						<div>
							<a class="layui-btn">病程记录</a>
							<a class="layui-btn">入院记录</a>
						</div>
						<div><a class="layui-btn">手术记录</a></div>
						<div>
							<select name="" id="">
								<option value="">护理评分</option>
								<option value="">疼痛评分</option>
							</select>
						</div>
					</div>
				</td>
			</tr>
		</table>
		<div class="minBtn">
			<a class="layui-btn">暂存</a>
			<a class="layui-btn">保存并返回</a>
			<a class="layui-btn">保存不返回</a>
