</body>
<script src="jquery-1.10.2.min.js"></script>
<script src="layui/layui.js"></script>
<script>
layui.use(['form','layedit'], function(){
  var form = layui.form;
  var layedit = layui.layedit;
   layedit.build('observation',{height:145});
 });

$(".moisture input").click(function(){
	$('.calculator').show();
})
$(".outMoisture input").click(function(){
	$('.outCalculator').show();
})
$(".outName input").click(function(){
	$('.outList').show();
})
$(".addTr").click(function(){
	var addTr = $(this).parent().parent().prev();
	var newTr = addTr.clone();
    newTr.insertBefore(addTr);
})

</script>
</html>